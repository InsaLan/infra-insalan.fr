{
    "pylint.cwd": "${workspaceFolder}/backend",
    "eslint.workingDirectories": [
        "frontend",
    ],
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue",
        "html",
        "vue-html",
        "css",
    ],
    "python.analysis.typeCheckingMode": "off",
}
